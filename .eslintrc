{
    "parser": "babel-eslint",
    "plugins": [
        "babel",
        "html"
      ],
      "settings": {
        "html/html-extensions": [".html", ".we"]
    },
    "rules": {
        "babel/new-cap": 1,
        "babel/camelcase": 0,
        "babel/no-invalid-this": 1,
        "babel/object-curly-spacing": 0,
        "babel/quotes": 0,
        "babel/semi": 1,
        "babel/no-unused-expressions": 1,
        "babel/valid-typeof": 1
    }
} 